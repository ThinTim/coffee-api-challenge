{
  "request": {
    "headers": {
      "Accept-Encoding": "gzip;q=1.0,deflate;q=0.6,identity;q=0.3",
      "Accept": "*/*"
    },
    "http_method": "post",
    "path": "/order/{coffee-name}"
  },
  "response": {
    "headers": {
      "Content-Type": "application/json"
    },
    "status": 201,
    "schema": {
      "$schema": "http://json-schema.org/draft-03/schema#",
      "description": "the schema for the POST order API.",
      "type": "object",
      "required": true,
      "properties": {
        "order": {
          "type": "string",
          "required": true
        }
      }
    }
  },
  "examples": {
    "default": {
      "request": {
        "uri": "http://localhost:4567/order/long-black"
      },
      "response": {
        "body": {
          "order": "/order/123"
        }
      }
    }
  }
}
